{
  "name": "xp_challenge",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:migrate:prod": "npx prisma migrate deploy && node dist/scripts/seeders/seeder.js && npm run start",
    "start": "node dist/main.js",
    "build": "tsc",
    "test:cov": "jest --coverage",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "truncate": "ts-node src/scripts/clean-up",
    "restore": "ts-node src/scripts/clean-up && npx prisma db seed",
    "dev": "ts-node-dev -r tsconfig-paths/register src/main.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moraeslua/xp_challenge.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/moraeslua/xp_challenge/issues"
  },
  "homepage": "https://github.com/moraeslua/xp_challenge#readme",
  "prisma": {
    "seed": "ts-node src/scripts/seeders/seeder.ts"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.5",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "prisma": "^4.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "@types/bcrypt": "^5.0.0",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "swagger-ui-express": "^4.5.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/*.spec.ts"
    ],
    "rootDir": "src",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ]
  }
}
